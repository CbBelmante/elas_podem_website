{
  "name": "elas_podem_website",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "cbcomponentsLinkLocal": "bash scripts/linkLocal.sh",
    "cbcomponentsLinkRemote": "bash scripts/linkRemote.sh",
    "cleanViteCache": "rm -rf .nuxt .output .vite node_modules/.vite node_modules/.cache",
    "lint": "eslint .",
    "lintFix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy": "npm run generate && firebase deploy --only hosting",
    "deployPreview": "npm run generate && npx serve .output/public",
    "seedAdmin": "npx tsx --no-deprecation scripts/seedAdmin.ts"
  },
  "dependencies": {
    "dayjs": "^1.11.19",
    "firebase": "^12.9.0",
    "lucide-vue-next": "^0.563.0",
    "nuxt": "^4.3.0",
    "reka-ui": "^2.7.0",
    "vue": "^3.5.27",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.6.4",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@inquirer/prompts": "^8.2.1",
    "@nuxt/eslint": "^1.13.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.39.2",
    "firebase-admin": "^13.6.1",
    "prettier": "^3.8.1"
  }
}
